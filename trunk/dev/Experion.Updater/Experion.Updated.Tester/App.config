<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IService1" />
                <binding name="BasicHttpBinding_IHealthService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://192.168.1.90:8080/EHS/HealthService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService1"
                contract="EHS.IService1" name="BasicHttpBinding_IService1" />
            <endpoint address="http://192.168.1.90:8080/EHS/HealthService.svc"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IHealthService"
                contract="EHS.IHealthService" name="BasicHttpBinding_IHealthService" />
        </client>
    </system.serviceModel>
</configuration>